# Export definitions for kwiver to be used from find_package
#include("${CMAKE_CURRENT_LIST_DIR}/arrows-darknet-config-targets.cmake")

# KWIVER version
set(ARROWS_DARKNET_VERSION_MAJOR @ARROWS_DARKNET_VERSION_MAJOR@)
set(ARROWS_DARKNET_VERSION_MINOR @ARROWS_DARKNET_VERSION_MINOR@)
set(ARROWS_DARKNET_VERSION_PATCH @ARROWS_DARKNET_VERSION_PATCH@)
set(ARROWS_DARKNET_VERSION "@ARROWS_DARKNET_VERSION@")


if (WIN32)
  set(SETUP_BATCH_FILES "@SETUP_BATCH_FILES@")
  add_definitions(-DBOOST_ALL_NO_LIB)
  add_definitions(-DBOOST_PROGRAM_OPTIONS_DYN_LINK)
  if(${MSVC_VERSION} GREATER_EQUAL 1915)
    # You must acknowledge that you understand MSVC
    # resolved a byte alignment issue in this compiler.
    # We get this due to using Eigen objects and
    # allocating those objects with make_shared
    add_definitions(-D_ENABLE_EXTENDED_ALIGNED_STORAGE)
  endif()
endif ()

set(ARROWS_DARKNET_DEFAULT_LIBRARY_DIR "@ARROWS_DARKNET_DEFAULT_LIBRARY_DIR@")
set(ARROWS_DARKNET_LIBRARY_DIR         "@ARROWS_DARKNET_BINARY_DIR@/@ARROWS_DARKNET_DEFAULT_LIBRARY_DIR@")
set(ARROWS_DARKNET_LIBRARIES           @arrows_darknet_libs@)
set(ARROWS_DARKNET_DOCUMENTATION_DIR   "@KWIVER_DOCUMENTATION_DIR@")
set(ARROWS_DARKNET_PLUGIN_DIR          "${ARROWS_DARKNET_LIBRARY_DIR}/@kwiver_plugin_algorithm_subdir@")

set(ARROWS_DARKNET_CMAKE_DIR           "@ARROWS_DARKNET_CMAKE_DIR@")

set(ARROWS_DARKNET_DIR "@ARROWS_DARKNET_BINARY_DIR@")
